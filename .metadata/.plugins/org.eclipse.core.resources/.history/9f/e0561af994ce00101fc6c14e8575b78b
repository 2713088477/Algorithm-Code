package Exp5;

import java.util.Scanner;

public class MainA {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		while(sc.hasNextInt()) {
			int k=sc.nextInt(),space=sc.nextInt();
			int[] value =new int[k];
			int[] weight=new int[k];
			for(int i=0;i<k;i++) {
				value[i]=sc.nextInt();
			}
			for(int i=0;i<k;i++) {
				weight[i]=sc.nextInt();
			}
			
		}
		
	
	}
	public static void f1(int k,int space,int[] value,int[] weight) {
		//dp[i][j]表示前i个中利用space能创造的最大价值
		int[][] dp=new int[k+1][space+1];
		for(int i=1;i<=k;i++) {
			for(int j=1;j<=space;j++) {
				dp[i][j]=dp[i-1][j];
				if(j-weight[i-1]>=0) {
					dp[i][j]=Math.max(dp[i][j], dp[i-1][j-weight[i-1]]+value[i-1]);
				}
			}
		}
		System.out.println(dp[k][space]);
		
	}

}
