package Exp5;

import java.util.Scanner;

public class Main_CreateArray {
	private static int n;
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		while(sc.hasNextInt()) {
			n=sc.nextInt();
			int[][] arr=new int[n][n];
			f(arr,0,0);
		}
	}
	public static void f(int[][] arr,int x,int y) {
		if(x==n-1 && y==n-1) {
			for(int[] t:arr) {
				for(int num:t) {
					System.out.print(num+" ");
				}
				System.out.println();
			}
		}
		outer:
		for(int i=1;i<=n;i++) {
			//查看当前行是否有
			for(int j=0;j<y;j++) {
				if(arr[x][i]==i) continue outer;
			}
			//查看当前列是否有
			for(int j=0;j<x;j++) {
				if(arr[j][y]==i) continue outer;
			}
			arr[x][y]=i;
			if(y==n-1) f(arr,x+1,0);
			else f(arr,x,y+1);
			arr[x][y]=0;
			
		}
	}
}
