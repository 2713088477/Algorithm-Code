package Exp4;

import java.util.Arrays;
import java.util.Collections;
import java.util.HashMap;
import java.util.Map;
import java.util.Scanner;
import java.util.StringJoiner;

public class Main_Save_Exe {
	static class Exe{
		int index;
		int value;
		@Override
		public int compareTo(Exe e) {
			if(this.value != e.value) {
				return Integer.compare(e.value, this.value);
			}else {
				return Integer.compare(this.index, e.index);
			}
		}
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		while(sc.hasNextInt()) {
			int len=sc.nextInt();
			int total=sc.nextInt();
			Exe[] arr=new Exe[len];
			for(int i=0;i<len;i++) {
				arr[i]=new Exe();
				arr[i].index=i+1;
				arr[i].value=sc.nextInt();
			}
			//排序
			Arrays.sort(arr);
			int ans=0;
			StringJoiner sj=new StringJoiner(",");
			for(Exe e:arr) {
				if(total-e.value>=0) {
					ans++;
					sj.add(String.valueOf(e.index));
				}else {
					break;
				}
			}
			System.out.println(ans);
			System.out.println(sj);
			
		}
	}

}
