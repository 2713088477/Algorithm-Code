package Exp5;

import java.util.Arrays;
import java.util.Scanner;

public class MainA {
	private static int n,b;
	private static int[] value,weight;
	private static int[] cnt,ansArr;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while(sc.hasNextInt()) {
			n=sc.nextInt();
			b=sc.nextInt();
			value=new int[n];
			weight=new int[n];
			for(int i=0;i<n;i++) {
				value[i]=sc.nextInt();
			}
			for(int i=0;i<n;i++) {
				weight[i]=sc.nextInt();
			}
			/*
			 * System.out.println(n+" "+b); System.out.println(Arrays.toString(value));
			 * System.out.println(Arrays.toString(weight));
			 */
			cnt=new int[n];
			ansArr=new int[n];
			System.out.println(findMax(b));
		}
		sc.close();
	}
	//从当前剩余的重量中能找到的最大价值
	public static int findMax(int leftWeight) {
		if(leftWeight==0) {
			return 0;
		}
		int ans=0;
		for(int i=0;i<n;i++) {
			if(cnt[i]==0 && weight[i]<=leftWeight) {
				cnt[i]=1;
				int sumTotal=findMax(leftWeight-weight[i])+value[i];
				if(sumTotal>ans) {
					ans=sumTotal;
				}
				cnt[i]=0;
			}
		}
		int current=0;
		for(int i=0;i<cnt.length;i++) {
			if(cnt[i]==1) {
				current+=value[i];
			}
		}
		if(current<ans) {
			ansArr
		}
		return ans;
	}

}
