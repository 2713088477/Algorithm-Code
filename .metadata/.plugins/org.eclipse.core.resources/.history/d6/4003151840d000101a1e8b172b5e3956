package Exp5;

import java.util.Scanner;

public class Main_pack_F {
	static int n,c1,c2;
	static int[] arr;
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		while(sc.hasNextInt()) {
			n=sc.nextInt();
			c1=sc.nextInt();
			c2=sc.nextInt();
			arr=new int[n];
			for(int i=0;i<n;i++) {
				arr[i]=sc.nextInt();
			}
			find(0,0,0);
		}
	}
	public static void find(int inc1,int inc2,int index) {
		if(index==n) {
			System.out.println("Yes");
			return;
		}
		if(inc1+arr[index]<=c1) {
			find(inc1+arr[index],inc2,index+1);
		}
		if(inc2+arr[index]<=c2) {
			find(inc1,inc2+arr[index],index+1);
		}
	}
}
