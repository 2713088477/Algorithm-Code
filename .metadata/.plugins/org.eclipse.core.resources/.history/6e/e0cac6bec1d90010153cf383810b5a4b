package dp;

import java.util.List;

public class SolutionLeetCode139 {
	public static void main(String[] args) {
    	String t="abc";
    	System.out.println(t.substring(3));
    }
    public boolean wordBreak(String s, List<String> wordDict) {
    	return false;
    }
    //匹配s是否能由wordDict组成
    public boolean isMatch(String s,List<String> wordDict) {
    	
    	for(String word:wordDict) {
    		if(s.startsWith(word)) {
    			if(isMatch(s.substring(word.length()),wordDict)) {
    				return true;
    			}
    		}
    	}
    	return false;
    }
    

}
